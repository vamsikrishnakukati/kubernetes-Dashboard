{
  "ignore": [
    "*", "docs", "i18n",
    ".cached_tools", ".git", ".github/ISSUE_TEMPLATE", ".tmp", "coverage", "cypress", ".idea",
    "aio/test-resources",
    "**/*.json", "**/*.yaml", "**/*.svg", "**/*.txt", "**/*.md", "**/OWNERS", "**/.helmignore", "**/*.lock"
  ],
  "license": "aio/scripts/license-header.txt",
  "defaultFormat": {
    "eachLine": {
      "prepend": "# "
    },
    "append": ""
  },
  "licenseFormats": {
    "go|ts|js|scss": {
      "eachLine": {
        "prepend": "// "
      },
      "append": ""
    },
    "html": {
      "prepend": "<!--",
      "append": "-->"
    }
  },
  "trailingWhitespace": "TRIM"
}
